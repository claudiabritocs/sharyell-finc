.dashboard_external
    background-color: #f3f3f3
    min-height: 900px

    .livewire_dash
        .implements
            padding-top: 30px
            padding-bottom: 30px
            background-color: #ddd9e2

            .center
                display: flex
                max-width: 1200px
                margin-left: auto
                margin-right: auto
                column-gap: 35px
                justify-content: space-between
                align-items: center
                @media (max-width: 1230px)  
                    flex-direction: column
                    row-gap: 24px
                @media (max-width: 550px)
                    row-gap: 60px

            .filters
                display: flex
                column-gap: 10px
                @media (max-width: 550px)
                    flex-wrap: wrap
                    justify-content: center
                    row-gap: 20px
                    padding: 0 20px
                    max-width: 360px

                .btn_search 
                    @media (max-width: 550px)
                        width: 80%
                        height: 35px
                    @media (max-width: 370px)
                        width: 50%

            .buttons
                position relative
                display: flex
                column-gap: 15px

                .pop_up_earn
                    position: fixed
                    width: 920px
                    background-color: $verdeMusgo
                    border-radius: 12px
                    z-index: 102
                    padding: 20px
                    left: 50%
                    top: 50%
                    border: 2px solid $verdeMusgo
                    transform: translate(-50%,-50%)
                    box-shadow: -1px 0px 6px 3px rgba(0,0,0,0.3)
                    @media (max-width: 1030px)
                        max-width: 420px
                    @media (max-width: 520px)
                        max-width: 90%

                    .over
                        display: flex
                        justify-content: space-between
                        max-width: 820px
                        @media (max-width: 1030px)
                            flex-direction: column
                            row-gap: 20px

                        .title
                            jakarta 18
                            color: $verdeMusgo
                            color: white
                            font-weight: 600

                        .checker
                            display: flex
                            column-gap: 10px
                            align-items: center
                            user-select: none

                    .fields
                        position: relative
                        display flex
                        column-gap: 10px
                        margin-top: 15px
                        @media(max-width: 1030px)
                            flex-wrap: wrap
                            row-gap: 15px

                        .closer
                            position: absolute
                            right: -13px
                            top: -54px
                            height: 28px
                            cursor: pointer
                            transition: all 0.4s
                            @media (max-width: 1030px)
                                top: -97px
                            &:hover
                                transform: scale(1.4)

                        .select_default
                            width: 170px
                            @media(max-width: 1030px)
                                width: 100%

                        .input_text
                            width: 380px
                            @media(max-width: 1030px)
                                width: 100%
                            &::placeholder
                                color: $verdeMedio
                                font-weight: 400

                        .number_live
                            position: relative
                            @media(max-width: 1030px)
                                width: 48%
                            @media (max-width: 390px)
                                width: 100%

                            .input_number
                                width: 140px
                                @media(max-width: 1030px)
                                    width: 100%
                                &::placeholder
                                    color: $verdeMedio
                                    font-weight: 400

                            p
                                jakarta 14
                                color: white

                        .date_input
                            @media(max-width: 1030px)
                                width: 48%
                            @media (max-width: 390px)
                                width: 100%

                .pop_up_spend
                    position: fixed
                    width: 920px
                    background-color: rgb(119, 61, 61)
                    border-radius: 12px
                    z-index: 102
                    padding: 20px
                    left: 50%
                    top: 50%
                    transform: translate(-50%,-50%)
                    box-shadow: -1px 0px 6px 3px rgba(0,0,0,0.3)
                    @media (max-width: 1030px)
                        max-width: 420px
                    @media (max-width: 520px)
                        max-width: 90%

                    .over
                        display: flex
                        justify-content: space-between
                        max-width: 820px
                        @media (max-width: 1030px)
                            flex-direction: column
                            row-gap: 20px

                        .title
                            jakarta 18
                            color: white
                            font-weight: 600

                        .checker
                            display: flex
                            column-gap: 10px
                            align-items: center
                            user-select: none

                    .fields
                        position: relative
                        display flex
                        column-gap: 10px
                        margin-top: 15px
                        @media(max-width: 1030px)
                            flex-wrap: wrap
                            row-gap: 15px

                        .closer
                            position: absolute
                            right: -13px
                            top: -54px
                            height: 28px
                            cursor: pointer
                            transition: all 0.4s
                            @media (max-width: 1030px)
                                top: -97px
                            &:hover
                                transform: scale(1.4)

                        .select_default
                            width: 170px
                            border: 1px solid #691212
                            color: $roxo
                            @media(max-width: 1030px)
                                width: 100%

                        .input_text
                            width: 380px
                            @media(max-width: 1030px)
                                width: 100%
                            &::placeholder
                                color: $roxo
                                font-weight: 400

                        .number_live
                            position: relative
                            color: $roxo
                            @media(max-width: 1030px)
                                width: 48%
                            @media (max-width: 390px)
                                width: 100%

                            .input_number
                                width: 140px
                                @media(max-width: 1030px)
                                    width: 100%
                                &::placeholder
                                    color: $roxo
                                    font-weight: 400   

                            p
                                jakarta 14
                                color: $roxo

                        .date_input
                            color: $roxo
                            @media(max-width: 1030px)
                                width: 48%
                            @media (max-width: 390px)
                                width: 100%

        .centre
            display block
            max-width: 1200px
            margin-left: auto
            margin-right: auto
            padding-top: 10px
            padding-bottom: 70px
            @media (max-width: 1230px)
                max-width: 90%

            .super_title
                jakarta 28
                font-weight: 600
                text-align: center

            .father
                display: flex
                padding-top: 35px
                justify-content: space-between
                column-gap: 25px
                row-gap: 60px
                flex-wrap: wrap
                @media (max-width: 1230px)   
                    flex-direction: column
                    align-items: center

                .card
                    background-color: white
                    box-shadow: 0px 1px 3px 1px rgba(0,0,0,0.2)
                    width: 405px
                    border-radius: 6px
                    @media (max-width: 550px) 
                        width: 100%
                        
                    .graph_free
                        width: 370px
                        height: 370px
                        margin: 0 auto
                        @media (max-width: 550px)
                            width: 270px
                            height: 270px

                        .no_data
                            jakarta 18
                            text-align: center

                    .infos
                        margin-top: 30px

                        .title
                            jakarta 18
                            font-weight: 600
                            text-align: center
                            padding: 0 20px
                            margin: 0
                .third_card

                    .extra_info
                        position: relative
                        width: 340px
                        background-color: white
                        height: min-content
                        box-shadow: 0px 1px 3px 1px rgba(0,0,0,0.2)
                        border-radius: 6px
                        margin-top: 24px
                        @media (max-width: 550px)
                            width: 270px

                        .bg_spent
                            position: absolute
                            padding: 10px 15px
                            z-index 2
                            background-color: rgb(148, 22, 22)
                            border-radius: 6px
                            width: 90%
                            top: -25px
                            left: 50%
                            transform: translateX(-50%)

                            .spacing
                                display: flex
                                justify-content: space-between
                                column-gap: 15px
                                @media (max-width: 550px)
                                    flex-direction: column
                                    align-items: center
                                    margin-bottom: 15px

                                .free_spent
                                    margin: 0
                                    jakarta 16
                                    color: white

                        .month_title
                            margin: 0
                            jakarta 16
                            padding-top: 80px
                            text-align: center
                            padding-bottom: 15px
                            @media (max-width: 550px)
                                padding-top: 200px

                    .goal
                        position: relative
                        width: 100%
                        background-color: white
                        height: min-content
                        box-shadow: 0px 1px 3px 1px rgba(0,0,0,0.2)
                        border-radius: 6px
                        margin-top: 24px
                        padding: 10px

                        .bar_box
                            position: relative
                            border: 1px solid $verdeMusgo
                            border-radius: 4px
                            height: 35px
                            padding: 5px

                            .bar
                                background-color: $verdeClaro
                                height: 100%

                            .percentage
                                position: absolute
                                z-index: 3
                                color: white
                                jakarta 14
                                left: 1px
                                top: 5px
                                text-align: center

                        .goal_add
                            display: flex
                            justify-content: space-between
                            align-items: center
                            margin-top: 7px

                            .number
                                jakarta 14
                                text-align: center
                                @media(max-width: 550px)
                                    font-size: 13px

                        .pop_up_goal
                            position: fixed
                            width: 500px
                            background-color: #1f2937
                            border-radius: 12px
                            z-index: 102
                            padding: 25px
                            left: 50%
                            top: 50%
                            transform: translate(-50%,-50%)
                            box-shadow: -1px 0px 6px 3px rgba(0,0,0,0.3)
                            @media (max-width: 1030px)
                                max-width: 420px
                            @media (max-width: 580px)
                                max-width: 80%
                            @media (max-width: 390px)
                                max-width: 90%

                            .title
                                jakarta 18
                                color: white
                                font-weight: 600
                                @media (max-width: 580px)
                                    padding-right: 30px

                            .fields
                                position: relative
                                display flex
                                column-gap: 10px
                                margin-top: 15px
                                @media(max-width: 1030px)
                                    flex-wrap: wrap
                                    row-gap: 15px

                                .closer
                                    position: absolute
                                    right: -13px
                                    top: -54px
                                    height: 28px
                                    cursor: pointer
                                    transition: all 0.4s
                                    @media (max-width: 472px)
                                        top: -83px
                                    &:hover
                                        transform: scale(1.4)

                                .number_live
                                    position: relative
                                    color: $roxo
                                    width: 100%

                                    .input_number
                                        width: 140px
                                        width: 100%
                                        &::placeholder
                                            color: $roxo
                                            font-weight: 400   

                                    p
                                        jakarta 14
                                        color: $roxo

                .card_liner
                    background-color: white
                    box-shadow: 0px 1px 3px 1px rgba(0,0,0,0.2)
                    width: 100%
                    border-radius: 6px
                    background-color: rgb(255, 233, 233)
                    
                    .infos
                        margin-top: 30px
                        margin-bottom: 0px

                        .title
                            jakarta 18
                            font-weight: 600
                            text-align: center
                            padding: 0 20px
                            margin: 0

                    .graph_liner
                        width: 100%
                        height: 300px
                        margin: 0 auto
                        background-color: rgb(255, 233, 233)

